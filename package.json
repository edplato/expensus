{
  "name": "expensus",
  "version": "0.0.1",
  "description": "Expenses for the modern family",
  "main": "index.js",
  "scripts": {
    "server": "ts-node-dev --no-notify lib/server",
    "webpack:watch": "webpack --config lib/client/config/webpack.config.ts --watch --colors",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck:server": "tsc --noEmit --project tsconfig.json",
    "typecheck:client": "tsc --noEmit --project tsconfig.client.json"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./test/test-setup.ts",
      "jest-localstorage-mock"
    ],
    "testURL": "https://sdk.loyaltylion.dev"
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredmeek/expensus.git"
  },
  "author": "Jared Meek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaredmeek/expensus/issues"
  },
  "homepage": "https://github.com/jaredmeek/expensus#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.5",
    "moment": "^2.29.4",
    "pg": "^7.4.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-act": "^1.7.1",
    "redux-thunk": "^2.2.0",
    "reflect-metadata": "^0.1.12",
    "reselect": "^3.0.1",
    "tslib": "^1.9.0",
    "typeorm": "^0.1.12"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/dotenv": "^4.0.2",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.1.2",
    "@types/koa": "^2.0.44",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-favicon": "^2.0.19",
    "@types/koa-logger": "^3.1.0",
    "@types/koa-router": "^7.0.27",
    "@types/koa-static": "^4.0.0",
    "@types/lodash": "^4.14.104",
    "@types/node": "^9.4.0",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-redux": "^5.0.15",
    "@types/webpack": "^3.8.8",
    "awesome-typescript-loader": "^3.4.1",
    "browser-env": "^3.2.5",
    "clean-webpack-plugin": "^0.1.18",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.9",
    "dotenv": "^5.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-matchers": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fork-ts-checker-webpack-plugin": "^0.3.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.2.2",
    "jest-localstorage-mock": "^2.2.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.12",
    "prettier": "^1.10.2",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "testdouble": "^3.5.0",
    "ts-jest": "^22.0.4",
    "ts-loader": "^3.5.0",
